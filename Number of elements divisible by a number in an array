#include <stdio.h>

int main()
{
    int n,d;
    scanf("%d",&n);
    int arr[n];
    for(int i=0;i<n;i++){
        scanf("%d",&arr[i]);
    }
    printf("enter the table :");
    scanf("%d",&d);
    int count =0;
    for(int i=0;i<n;i++){
        if(arr[i]%d==0)
        count++;
    }
    printf("%d",count);

    return 0;
}
