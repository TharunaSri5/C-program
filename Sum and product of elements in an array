#include <stdio.h>

int main()
{
    int n;
    
    scanf("%d",&n);
    int arr[n];
    for(int i=0;i<n;i++){
        scanf("%d",&arr[i]);
    }
    int sum=0;
    int pdt=1;
    for(int i=0;i<n;i++){
        sum+=arr[i];
    }
    for(int i=0;i<n;i++){
        pdt*=arr[i];
    }
    printf("%d\n",sum);
    printf("%d ",pdt);
    

    return 0;
}
