#include <stdio.h>

int main()
{
    int n;
    int sum=0;
    scanf("%d",&n);
    int arr[n];
    for(int i=0;i<n;i++){
        scanf("%d",&arr[i]);
    }
    int count;
    for(int i=0;i<n;i++){
        count=0;
        for(int j=i+1;j<n;j++){
            if(arr[i]==arr[j])
            count++;
        }
        if(count!=0){
        printf("%d ",arr[i]);
        sum=sum+arr[i];
        }
    }
        printf("%d",sum);
    
    
    
        
      
    
  
    

    return 0;
}
